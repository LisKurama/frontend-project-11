{
  "env": {
    "browser": true,
    "node": false,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-param-reassign": ["error", { "props": false }],
    "no-console": "off",
    "no-undef": "error",
    "no-use-before-define": ["error", { "functions": false, "classes": true }],
    "import/extensions": ["error", "ignorePackages", { "js": "always" }]
  }
}
  